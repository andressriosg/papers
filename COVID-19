             size = 1, color = "firebrick1") + 
  labs(x = 'Date', y = 'Number of infected population')  + 
  scale_x_date(date_breaks = "4 months", date_labels = "%b/%y") 

grafica_covidreingles <- ggplot(data = datos_finales_covid) + 
  geom_point(aes(x = datos_finales_covid[, 1], y = datos_finales_covid[, 3]), 
             size = 1, color = "#698B69") + 
  labs(x = 'Date', y = 'Number of recovered population')  + 
  scale_x_date(date_breaks = "4 months", date_labels = "%b/%y")

grafica_covidfaingles <- ggplot(data = datos_finales_covid) + 
  geom_point(aes(x = datos_finales_covid[, 1], y = datos_finales_covid[, 4]), 
             size = 1, color = "#6C7B8B") + 
  labs(x = 'Date', y = 'Number of deceased population')  + 
  scale_x_date(date_breaks = "4 months", date_labels = "%b/%y")

grafica_covidvaingles <- ggplot() + 
  geom_line(data = vacunacovid, aes(x = vacunacovid[, 1], y = vacunacovid[, 2]), 
            linewidth = 1, color = "#00F5FF") +
  geom_line(data = datos_finales_covid, 
            aes(x = datos_finales_covid[, 1], y = datos_finales_covid[, 5]), 
            linewidth = 1, color = "#00C5CD") +
  labs(x = 'Date', y = 'Number of vaccinated population')  + 
  scale_x_date(date_breaks = "4 months", date_labels = "%b/%y") 

grafica_covidhoingles <- ggplot() + 
  geom_point(data = datos_finales_covid, 
             aes(x = datos_finales_covid[,1], y = datos_finales_covid[,6]), 
             size = 1, color = "#FF1493") + 
  geom_line(data = df_uci_gg , 
            aes(x = df_uci_gg[, 1], y = df_uci_gg[, 2]), 
            linewidth =  1, color = "#FF1493") +
  labs(x = 'Date', y = 'Number of hospitalized population')  + 
  scale_x_date(date_breaks = "4 months", date_labels = "%b/%y") 

