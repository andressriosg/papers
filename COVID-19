  geom_line(aes(x = df_R0covidingles[, 1], y = df_R0covidingles[, 2]), 
            linewidth = 1, color = "purple3") + 
  labs(x = 'Date', y = bquote("Estimated " ~ R[0])) + 
  scale_x_date(date_breaks = "4 months", date_labels = "%b/%y") + 
  scale_y_continuous(breaks = seq(0, 1.2, 0.2), limits = c(0, 1.15))

## Prueba de hipotesis 

Dc2 = (R0covid[4:652])^2/var_R0covid[4:652]
valoresp = rep(0, 652)
for (i in 4:652){
  valoresp[i] = pchisq(Dc2[i], df = 1, ncp = 1/var_R0covid[i])
}

plot(valoresp[4:652])
abline(h = 0.05, lwd = 2, col = "red")

valoresp1 <- data.frame(datos_finales_covid[4:652, 1], 
                        valoresp[4:652])

graficavaloresp <- ggplot(data = valoresp1) + 
  geom_line(aes(x = valoresp1[, 1], y = valoresp1[, 2]), 
            linewidth = 1, color = "darkolivegreen") + 
  geom_hline(yintercept = 0.05, color = "red", linetype = "dashed", linewidth = 0.8) + 
  labs(x = 'Date', y = "p-value")  + 
  scale_x_date(date_breaks = "4 months", date_labels = "%b/%y")

graficavaloresp
